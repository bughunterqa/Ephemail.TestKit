<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>

		<Version>1.1.1</Version>
		<PackageId>EphemailClient</PackageId>
		<Authors>Vladyslav Rybalka</Authors>
		<Company>Ephemail</Company>
		<Description>Client for Ephemail API with email extraction, template matching and automation support.</Description>
		<PackageTags>ephemail email automation qa test email-client</PackageTags>
		<RepositoryUrl>https://github.com/YOUR_USERNAME/ephemail</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="System.Net.Http.Json" Version="9.0.5" />
	</ItemGroup>

</Project>
